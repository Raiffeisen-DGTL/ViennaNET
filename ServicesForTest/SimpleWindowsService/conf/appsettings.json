{
  "webApiConfiguration": {
    "portNumber": 9001,
    "logRequests": true,
    "swaggerSubmit": true
  },
  "metrics": {
    "enabled": true,
    "reporter": "prometheus"
  },
  "webApiStaticFiles": {
    "urlPrefix": "public",
    "folder": "Public",
    "cacheInterval": 60000
  },
  "logger": {
    "listeners": [
      {
        "type": "console",
        "category": "All",
        "minLevel": "Debug"
      },
      {
        "type": "textFile",
        "category": "All",
        "minLevel": "Debug",
        "params": {
          "maxSize": "50000",
          "append": "append",
          "rollBackBackups": "10",
          "filePatternName": "Logs\\'XmlEngine_Rest'_yyyy.MM.dd.log"
        }
      },
      {
        "type": "textFile",
        "category": "All",
        "minLevel": "Error",
        "params": {
          "maxSize": "50000",
          "append": "append",
          "rollBackBackups": "10",
          "filePatternName": "Logs\\'XmlEngine_Rest'_yyyy.MM.dd.err"
        }
      }
    ]
  }
}
