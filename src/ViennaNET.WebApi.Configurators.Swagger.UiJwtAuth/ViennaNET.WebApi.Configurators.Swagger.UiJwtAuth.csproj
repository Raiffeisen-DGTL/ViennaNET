<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\net6.0\ViennaNET.WebApi.Configurators.Swagger.UiJwtAuth.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\net6.0\ViennaNET.WebApi.Configurators.Swagger.UiJwtAuth.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="swaggerAuth.js" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="swaggerAuth.js">
      <CopyToOutputDirectory>Never</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\ViennaNET.WebApi.Abstractions\ViennaNET.WebApi.Abstractions.csproj" />
    <ProjectReference Include="..\..\src\ViennaNET.WebApi.Configurators.Swagger\ViennaNET.WebApi.Configurators.Swagger.csproj" />
  </ItemGroup>
  
</Project>
